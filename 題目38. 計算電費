#include<stdio.h>  
#include<stdlib.h>  
  
int main(){  
   double aa=0; 
   double bb=0; 
   double cc=0; 
   double dd=0; 
   double ee=0; 
   double ff=0; 
   double gg=0; 
   double hh=0; 
   double ii=0; 
   double jj=0; 
    
   int a=0;  
   double b=2.10; 
   double c=3.02; 
   double d=4.39; 
   double e=4.97; 
   double f=5.63; 
   double g=2.10; 
   double h=2.68; 
   double i=3.61; 
   double j=4.01; 
   double k=4.50;   
    
while( scanf("%d",&a)!=EOF ) {  
  if(a<=120){ 
      aa=a*b; 
    printf("Summer months:%.2lf\n",aa); 
    bb=a*g; 
    printf("Non-Summer months:%.2lf\n",bb); 
  } 
  else if(a>=121 && a<=330){ 
     cc=(a-120)*c+252; 
      printf("Summer months:%.2lf\n",cc); 
    dd=(a-120)*h+252; 
      printf("Non-Summer months:%.2lf\n",dd); 
  } 
  else if(a>=331 && a<=500){ 
     ee=(a-330)*d+252+634.2; 
    printf("Summer months:%.2lf\n",ee); 
    ff=(a-330)*i+252+562.8; 
    printf("Non-Summer months:%.2lf\n",ff); 
  } 
  else if(a>=501 && a<=700){ 
     gg=(a-500)*e+252+634.2+746.3; 
      printf("Summer months:%.2lf\n",gg); 
    hh=(a-500)*j+252+562.8+613.7; 
      printf("Non-Summer months:%.2lf\n",hh); 
  } 
  else if(a>=701){ 
      ii=(a-700)*f+252+634.2+746.3+994; 
      printf("Summer months:%.2lf\n",ii); 
    jj=(a-700)*k+252+562.8+613.7+802; 
      printf("Non-Summer months:%.2lf\n",jj); 
  } 
 }   
   return 0;  
}  

